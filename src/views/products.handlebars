<h1> Productos</h1>
{{#each productos}}
<div>
    <h2> {{title}} </h2>
    <p> {{_id}} </p>
    <p> {{description}}</p>
    <p>Precio: ${{price}} </p>
    <p>Categoria: {{category}}</p>
    <button>Agregar</button>
</div>
{{/each}}

{{#if hasNextPage}}
<a href="/products?page={{nextPage}}">Siguiente</a>
{{/if}}

{{#if hasPrevPage}}
<a href="/products?page={{prevPage}}">Anterior</a>
{{/if}}

<p>Pagina {{currentPage}} de {{totalPage}}</p>